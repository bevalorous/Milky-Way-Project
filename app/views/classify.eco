<% translate = require 't7e' %>

<div class="interface">
  <div class="subject-options">
    <div><%- translate 'a.discuss', '<i class="fa fa-comments"></i>', title: 'classify.options.discuss' %></div>
    <div>
      <%- translate 'button', '<i class="fa fa-key"></i>', name: 'sign-in', title: 'classify.options.signIn' %>
      <%- translate 'button', '&#9733;', name: 'favorite', title: 'classify.options.favourite', disabled: true, style: 'display: none;' %>
    </div>
    <% helpButtonContent = '<i class="fa fa-question hide-with-overlay"></i>' %>
    <% helpButtonContent += '<span class="show-inline-with-overlay">&times;</span>' %>
    <div><%- translate 'button', helpButtonContent, name: 'help', title: 'classify.options.help' %></div>
  </div>

  <div class="subject">
    <!--Marking surface goes here-->
  </div>

  <div class="question"><%- translate 'span', 'classify.question' %></div>

  <div class="tools">
    <div class="tool">
      <label>
        <input type="radio" name="tool" value="bubble" checked="checked" />
        <div class="outer-label">
          <div class="inner-label">
            <div class="icon">
              <svg viewBox="0 0 40 40">
                <g>
                  <path fill="rgba(255, 255, 255, 0.5)" stroke="currentColor" stroke-width="1.25" stroke-linecap="round" stroke-miterlimit="10" d="M19.5,4.5c8.284,0,15,6.716,15,15
